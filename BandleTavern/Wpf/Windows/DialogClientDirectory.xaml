<Window x:Class="BandleTavern.Wpf.Windows.DialogClientDirectory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BandleTavern.Wpf.Windows"
        mc:Ignorable="d"
        Title="League Client Directory Entry" Height="213" Width="550" MinHeight="213" MinWidth="550" MaxHeight="213">
    <Grid Background="{StaticResource BrushBannerMainBackground}">
        <TextBox x:Name="textBox" Height="23" Margin="173,80,10,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock" Style="{StaticResource StyleTextBlockGeneral}" HorizontalAlignment="Left" Margin="10,80,0,0" TextWrapping="Wrap" Text="League Client Directory:" VerticalAlignment="Top" Height="23"/>
        <Button x:Name="buttonDone" Content="Done" HorizontalAlignment="Center" Margin="0,0,0,10" VerticalAlignment="Bottom" Width="76" Click="ButtonDone_Click"/>

    </Grid>
</Window>
