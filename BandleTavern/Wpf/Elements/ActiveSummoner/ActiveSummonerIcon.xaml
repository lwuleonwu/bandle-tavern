<UserControl x:Class="BandleTavern.Wpf.Elements.ActiveSummoner.ActiveSummonerIcon"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BandleTavern.Wpf.Elements.ActiveSummoner"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="450">
    <Grid Name="MainGrid">
        <!--Circular frame for summoner icon.-->
        <Ellipse>
            <Ellipse.Fill>
                <ImageBrush x:Name="SummonerIcon"/>
            </Ellipse.Fill>
        </Ellipse>
    </Grid>
</UserControl>
